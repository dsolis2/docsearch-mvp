# Google Drive API Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here  
GOOGLE_REFRESH_TOKEN=your_google_refresh_token_here

# VoyageAI API Key
VOYAGE_API_KEY=your_voyage_api_key_here

# Database Connection
DATABASE_URL=postgresql://localhost/docsearch_rag

# Optional Settings
EMBEDDING_MODEL=voyage-2
EMBEDDING_DIM=1024
CHUNK_SIZE=1200
CHUNK_OVERLAP=200
EMBED_QPS=4.0
EMBED_CONCURRENCY=2
LOG_LEVEL=INFO

# File Filtering (comma-separated)
INCLUDE_MIME_TYPES=application/pdf,application/vnd.google-apps.presentation